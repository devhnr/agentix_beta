    <script>
  var acc = document.getElementsByClassName("v-accordion");
  var i;
  
  var preValue = ''
  for (i = 0; i < acc.length; i++) {
    acc[i].addEventListener("click", function(event) {
      var activeList = document.getElementsByClassName("active");
      if (activeList.length >= 2) {
        for (let i = 0; i < activeList.length; i++) {
          if (activeList[i].classList[0] != 'back-to-top'){
            if ( this.textContent != preValue) {
              activeList[i].nextElementSibling.style.maxHeight = null;
              activeList[i].classList.remove("active");
              var panel = this.nextElementSibling;
              this.classList.add('active')
              preValue = this.textContent;
              panel.style.maxHeight = panel.scrollHeight + "px";
            }
            else {
              activeList[i].nextElementSibling.style.maxHeight = null;
              activeList[i].classList.remove("active");         
            }
          }
        }
      }
      else {
          var panel = this.nextElementSibling;
          this.classList.add('active')
          preValue = this.textContent;
          panel.style.maxHeight = panel.scrollHeight + "px";
      }
    });
  
  }
  </script>
  

